{
	"coffeescript_error": {
		"level": "error"
	},
	"arrow_spacing": {
		"level": "warn"
	},
	"no_tabs": {
		"level": "ignore"
	},
	"no_trailing_whitespace": {
		"level": "warn",
		"allowed_in_comments": false,
		"allowed_in_empty_lines": true
	},
	"max_line_length": {
		"value": 80,
		"level": "warn",
		"limitComments": true
	},
	"line_endings": {
		"level": "warn",
		"value": "unix"
	},
	"no_trailing_semicolons": {
		"level": "error"
	},
	"indentation": {
		"value": 1,
		"level": "error"
	},
	"camel_case_classes": {
		"level": "error"
	},
	"colon_assignment_spacing": {
		"level": "warn",
		"spacing": {
			"left": 0,
			"right": 1
		}
	},
	"no_implicit_braces": {
		"strict": true
	},
	"no_plusplus": {
	},
	"no_throwing_strings": {
		"level": "error"
	},
	"no_backticks": {
		"level": "error"
	},
	"no_implicit_parens": {
	},
	"no_empty_param_list": {
		"level": "warn"
	},
	"no_stand_alone_at": {
	},
	"space_operators": {
		"level": "warn"
	},
	"duplicate_key": {
		"level": "error"
	},
	"empty_constructor_needs_parens": {
	},
	"cyclomatic_complexity": {
		"value": 10
	},
	"newlines_after_classes": {
	},
	"no_unnecessary_fat_arrows": {
		"level": "warn"
	},
	"missing_fat_arrows": {
	},
	"non_empty_constructor_needs_parens": {
	}
}
